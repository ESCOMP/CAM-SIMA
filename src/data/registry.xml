<?xml version="1.0" encoding="UTF-8"?>

<registry name="cam_registry" version="1.0">
  <file name="physics_types" type="module">
    <use module="ccpp_kinds" reference="kind_phys"/>
    <variable local_name="ncol" standard_name="horizontal_dimension"
              units="count" type="integer" access="protected">
      <long_name>Number of horizontal columns</long_name>
      <initial_value>0</initial_value>
    </variable>
    <variable local_name="latitude" standard_name="latitude"
              units="radians" type="real" kind="kind_phys"
              allocatable="pointer" access="protected">
      <dimensions>horizontal_dimension</dimensions>
    </variable>
    <variable local_name="longitude" standard_name="longitude"
              units="radians" type="real" kind="kind_phys"
              allocatable="pointer" access="protected">
      <dimensions>horizontal_dimension</dimensions>
    </variable>
    <variable local_name="ps" standard_name="surface_pressure"
              units="Pa" type="real" kind="kind_phys"
              allocatable="pointer">
      <long_name>surface pressure</long_name>
      <dimensions>horizontal_dimension</dimensions>
    </variable>
    <variable local_name="psdry" standard_name="dry_surface_pressure"
              units="Pa" type="real" kind="kind_phys"
              allocatable="pointer">
      <long_name>surface pressure</long_name>
      <dimensions>horizontal_dimension</dimensions>
    </variable>
    <variable local_name="phis" standard_name="surface_geopotential"
              units="m2 s-2" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension</dimensions>
    </variable>
    <variable local_name="T" standard_name="temperature"
              units="K" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="u" standard_name="eastward_wind"
              units="m s-1" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="v" standard_name="northward_wind"
              units="m s-1" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="s" standard_name="dry_static_energy"
              units="J" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="omega" standard_name="vertical_pressure_velocity"
              units="Pa s-1" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="pmid" standard_name="midpoint_pressure"
              units="Pa" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="pmiddry" standard_name="midpoint_pressure_dry"
              units="Pa" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="pdel" standard_name="layer_thickness"
              units="Pa" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="pdeldry" standard_name="layer_thickness_dry"
              units="Pa" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="rpdel" standard_name="reciprocal_of_layer_thickness"
              units="Pa" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="rpdeldry"
              standard_name="reciprocal_layer_thickness_dry"
              units="Pa" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="lnpmid" standard_name="log_midpoint_pressure"
              units="None" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="lnpmiddry" standard_name="log_midpoint_pressure_dry"
              units="Pa" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="exner" standard_name="inverse_exner_function"
              units="None" type="real" kind="kind_phys"
              allocatable="pointer">
      <long_name>inverse exner function w.r.t. surface pressure (ps/p)^(R/cp)</long_name>
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="zm"
              standard_name="geopotential_height_above_surface_at_midpoints"
              units="m" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension   vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="q" standard_name="constituent_array"
              units="kg kg-1"
              type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension
      vertical_layer_dimension
      number_of_constituents</dimensions>
    </variable>
    <variable local_name="pint" standard_name="air_pressure_at_interface"
              units="Pa"
              type="real" kind="kind_phys"
              allocatable="pointer">
      <long_name>Air pressure at model layer interfaces</long_name>
      <dimensions>horizontal_dimension vertical_interface_dimension</dimensions>
    </variable>
    <variable local_name="pintdry" standard_name="dry_air_pressure_at_interface"
              units="Pa"
              type="real" kind="kind_phys"
              allocatable="pointer">
      <long_name>Dry air pressure at model layer interfaces</long_name>
      <dimensions>horizontal_dimension vertical_interface_dimension</dimensions>
    </variable>
    <variable local_name="lnpint"
              standard_name="natural_log_of_air_pressure_at_interface"
              units="1"
              type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_interface_dimension</dimensions>
    </variable>
    <variable local_name="lnpintdry"
              standard_name="natural_log_of_dry_air_pressure_at_interface"
              units="1"
              type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_interface_dimension</dimensions>
    </variable>
    <variable local_name="zi"
              standard_name="geopotential_height_above_surface_at_interfaces"
              units="m"
              type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_interface_dimension</dimensions>
    </variable>
    <variable local_name="te_ini"
              standard_name="vertically_integrated_total_energy_of_initial_state"
              units="J"
              type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension</dimensions>
      <long_name>Vertically integrated total (kinetic + static) energy of initial state</long_name>
    </variable>
    <variable local_name="te_cur"
              standard_name="vertically_integrated_total_energy_of_current_state"
              units="J"
              type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension</dimensions>
      <long_name>Vertically integrated total (kinetic + static) energy of current state</long_name>
    </variable>
    <variable local_name="tw_ini"
              standard_name="vertically_integrated_total_water_of_initial_state"
              units="J"
              type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension</dimensions>
    </variable>
    <variable local_name="tw_cur"
              standard_name="vertically_integrated_total_water_of_current_state"
              units="J"
              type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension</dimensions>
    </variable>
    <variable local_name="dTdt" standard_name="temperature_tendency"
              units="K s-1" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="dudt" standard_name="eastward_wind_tendency"
              units="m s-2" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <variable local_name="dvdt" standard_name="northward_wind_tendency"
              units="m s-2" type="real" kind="kind_phys"
              allocatable="pointer">
      <dimensions>horizontal_dimension vertical_layer_dimension</dimensions>
    </variable>
    <ddt type="physics_state">
      <data dycore="FV,EUL">surface_pressure</data>
      <data dycore="SE">dry_surface_pressure</data>
      <data>surface_geopotential</data>
      <data>temperature</data>
      <data>eastward_wind</data>
      <data>northward_wind</data>
      <data>vertical_pressure_velocity</data>
      <data dycore="EUL,FV">midpoint_pressure</data>
      <data dycore="SE">midpoint_pressure_dry</data>
      <data dycore="EUL,FV">layer_thickness</data>
      <data dycore="SE">layer_thickness_dry</data>
      <data>constituent_array</data>
    </ddt>
    <ddt type="physics_tend">
      <data>temperature_tendency</data>
      <data>eastward_wind_tendency</data>
      <data>northward_wind_tendency</data>
    </ddt>
    <variable local_name="phys_state"
              standard_name="physics_state_from_dynamics"
              units="None" type="physics_state">
      <long_name>Physics state variables updated by dynamical core</long_name>
    </variable>
    <variable local_name="phys_tend"
              standard_name="total_physics_tendency"
              units="None" type="physics_tend">
      <long_name>Total tendency from physics suite</long_name>
    </variable>
  </file>
</registry>
