<!-- Vertical Coordinate -->

  <entry id="pver">
    <type>integer</type>
    <category>vert_coord</category>
    <group>vert_coord_nl</group>
    <desc>
      Number of model vertical levels.
      Default: 30

      Please note that changing this variable
      will likely change other namelist variables
      via the "nlev" attribute.
    </desc>
    <values>
      <value>30</value>
      <value phys_suite="cam4">26</value>
      <value phys_suite="cam5">30</value>
      <value phys_suite="cam6">32</value>
      <value dyn="mpas" analytic_ic="1">32</value>
    </values>
  </entry>

<!-- Debug flag -->

  <entry id="debug_output">
    <type>integer</type>
    <category>diagnostics</category>
    <group>cam_logfile_nl</group>
    <valid_values>0,1,2,3</valid_values>
    <desc>
      Include extra checks and logging output.
      0: No extra checks or output
      1: Extra informational output
      2: Level 1 plus extra checks and informational output
      3: All available checks and output
    </desc>
    <values>
      <value>0</value>
      <value debug="1">2</value>
    </values>
  </entry>

