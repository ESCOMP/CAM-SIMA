<ccpp_datatable version="1.0">
  <schemes>
    <scheme name="test_scheme">
      <test_suite_physics filename="test_suite.xml" module="test_scheme" name="test_scheme" subroutine_name="test_scheme_run">
        <call_list>
          <var intent="in" name="horizontal_loop_extent" />
          <var intent="in" name="vertical_layer_dimension" />
          <var intent="in" name="latitude" />
          <var intent="in" name="longitude" />
          <var intent="in" name="eastward_wind"/>
          <var intent="in" name="northward_wind"/>
        </call_list>
      </test_suite_physics>
    </scheme>
  </schemes>
  <api>
    <suites>
      <suite filename="test_suite.xml" name="test_suite">
        <group name="test_suite_physics" phase="run">
            <scheme name="test_scheme" />
        </group>
      </suite>
    </suites>
  </api>
  <var_dictionaries>
    <var_dictionary name="ccpp_physics_api" parent="HelloWorld" type="api">
      <sub_dictionaries>test_suite</sub_dictionaries>
      <variables />
    </var_dictionary>
    <var_dictionary name="test_suite" type="suite">
      <sub_dictionaries>test_suite_physics</sub_dictionaries>
       <variables />
    </var_dictionary>
    <var_dictionary name="test_suite_physics_call_list" type="group_call_list">
      <variables>
        <var intent="in" kind="" local_name="ncol" name="horizontal_dimension" type="integer" units="count" />
        <var intent="in" kind="" local_name="pver" name="vertical_layer_dimension" type="integer" units="count" />
        <var intent="in" kind="" local_name="latitude" name="latitude" type="integer" units="count" />
        <var intent="in" kind="" local_name="longitude" name="longitude" type="integer" units="count" />
        <var intent="in" kind="kind_phys" local_name="u" name="eastward_wind" type="real" units="m s-1" />
        <var intent="in" kind="kind_phys" local_name="v" name="northward_wind" type="real" units="m s-1" />
      </variables>
    </var_dictionary>
  </var_dictionaries>
</ccpp_datatable>
